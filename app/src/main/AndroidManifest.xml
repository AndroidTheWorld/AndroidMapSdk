<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="chen.gddemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">



        <activity android:name=".LauncherActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name=".LocationActivity" />
        <activity android:name=".PoiSearchActivity" />
        <activity android:name=".GeoCoderActivity" />
        <activity android:name=".BasicMapActivity" />
        <activity android:name=".CircleMapActivity" />
        <activity android:name=".DriverRouteMapActivity" />
        <activity android:name=".LineMapActivity" />
        <activity android:name=".MarkerMapActivity" />

    </application>

</manifest>